{
  "Function": {
    "prefix": "funS",
    "body": ["function $1(){", "  $3", "}$2"],
    "description": "插入函式"
  },
  // Console Log
  "Console Log": {
    "prefix": "conS",
    "body": ["console.log($1);"],
    "description": "插入 console.log();"
  },
  // 判斷
  "If": {
    "prefix": "ifS",
    "body": ["if ($2) {", "  $3", "}$1"],
    "description": "插入 if 判斷"
  },
  "Else": {
    "prefix": "elseS",
    "body": ["else {", "  $2", "}$1"],
    "description": "插入 if 的 else 判斷"
  },
  "Else If": {
    "prefix": "elseifS",
    "body": ["else if ($2) {", "  $3", "}$1"],
    "description": "插入 if 的 else if 判斷"
  },
  // 插入三元運算子
  "Ternary Operator": {
    "prefix": "3S",
    "body": ["$1 == $2 ? $3 : $4;"],
    "description": "插入三元運算子"
  },
  // DOM 獲取
  "Document QuerySelector": {
    "prefix": "domS",
    "body": ["document.querySelector('$1')"],
    "description": "DOM 獲取"
  }
}
